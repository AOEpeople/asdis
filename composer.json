{
  "name": "aoe/asdis",
  "description": "Distributes on page assets onto multiple servers.",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "AOE GmbH",
      "email": "dev@aoe.com"
    }
  ],
  "support": {
    "email": "dev@aoe.com",
    "issues": "https://github.com/AOEpeople/asdis/issues",
    "source": "https://github.com/AOEpeople/asdis"
  },
  "require": {
    "php": "^7.0",
    "typo3/cms-core": ">=8.7.30, <= 10.4.99",
    "typo3/cms-extbase": ">=8.7.30, <= 10.4.99",
    "typo3/cms-backend": ">=8.7.30, <= 10.4.99",
    "typo3/cms-reports": ">=8.7.30, <= 10.4.99",
    "typo3/cms-frontend": ">=8.7.30, <= 10.4.99"
  },
  "require-dev": {
    "nimut/testing-framework": "^5.1"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "autoload": {
    "psr-4": {
        "Aoe\\Asdis\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aoe\\Asdis\\Tests\\": "Tests/"
    }
  },
  "scripts": {
    "test:unit": [
      "[ -e '.Build/bin/phpunit' ] || composer update",
      "TYPO3_PATH_ROOT=$(pwd)/.Build/Web .Build/bin/phpunit --colors -c .Build/vendor/nimut/testing-framework/res/Configuration/UnitTests.xml Tests"
    ]
  }
}
