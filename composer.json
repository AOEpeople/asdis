{
  "name": "aoe/asdis",
  "description": "Distributes on page assets onto multiple servers.",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "AOE GmbH",
      "email": "dev@aoe.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "typo3/cms-core": ">=6.2.0",
    "typo3/cms-extbase": ">=6.2.0",
    "typo3/cms-sv": ">=6.2.0",
    "typo3/cms-backend": ">=6.2.0",
    "typo3/cms-reports": ">=6.2.0",
    "typo3/cms-frontend": ">=6.2.0"
  },
  "require-dev": {
    "typo3/cms": "^7.6",
    "nimut/testing-framework": "^2.0.0",
    "phpunit/phpcov": "2.0.*",
    "squizlabs/php_codesniffer": "2.3.*",
    "sebastian/phpcpd": "2.0.*",
    "phpmd/phpmd": "2.2.*"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "autoload": {
    "classmap": ["Classes/", "Tests"]
  }
}
